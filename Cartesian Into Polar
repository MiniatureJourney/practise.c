#include<stdio.h>
#include<math.h>
int main()
{
	float x,y,r,angle,a,b;
	printf("enter two coordinates X and Y respectively");
	scanf("%f %f",&x,&y);

r=sqrt((x*x)+(y*y));
angle=atan(y/x);
angle=angle*0.01745
a=r*cos(angle);
b=r*sin(angle);




	printf("X and Y in polar coordinates are=%f %f",a,b);
	return 0;
	
}
