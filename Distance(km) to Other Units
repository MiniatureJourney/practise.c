#include<stdio.h>
int main()
{
	float km,m,cm,feet,in;
	printf("Enter Distance between two cities(km)= ");
	scanf("%f",&km);
	
	m=1000*km;
	printf("distance in (M) is=%f\n",m);
	
	cm=100000*km;
	printf("distance in (CM) is=%f\n",cm);
	
	feet=km*3280.84;
	printf("distance in (CM) is=%f\n",feet);
	
	in=39370.1*km;
		printf("distance in (IN) is=%f\n",in);
		
		return 0;
		}
	
	
	
