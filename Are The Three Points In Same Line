#include<stdio.h>
int main()
{
float x1,y1,x2,y2,x3,y3,slope1,slope2;
printf("enter the first two coordinates");
scanf("%f %f",&x1,&x2);
printf("enter the first 2nd coordinates");
scanf("%f %f",&x2,&y2);
printf("enter the first 3rd coordinates");
scanf("%f %f",&x3,&y3);
slope1=(y2-y1)/(x2-x1);
slope2=(y3-y2)/(x3-x2);
if(slope1==slope2)
printf("three points are in the same line");
else
printf("three points are not in the same line");
return 0;
}


