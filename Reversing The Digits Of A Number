#include<stdio.h>
int main()
{
	int num,d1,d2,d3,d4,d5,d6;
	printf("Enter the number to be reversed");
	scanf("%d",&num);
	d6=num%10;
	num=num/10;
		d5=num%10;
	num=num/10;
	
		d4=num%10;
	num=num/10;
	
		d3=num%10;
	num=num/10;
	
		d2=num%10;
	num=num/10;
	
		d1=num%10;
	num=num/10;
	
	
	num=d6*100000+d5*10000+d4*1000+d3*100+d2*10+d1;
	printf("obrtained reverse number is=%d",num);
	return 0;
	
}
